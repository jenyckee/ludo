{
  "name": "ludojs",
  "version": "0.0.11",
  "description": "Webpack seed",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "author": "Jeroen Nyckees",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "watch": "webpack src/index.ts lib/index.js --watch",
    "build": "npm run build:commonjs && npm run build:umd",
    "build:umd": "cross-env BABEL_ENV=commonjs webpack src/index.js dist/ludo.js",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.14.0",
    "browser-sync": "^2.8.0",
    "cross-env": "^3.1.4",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.8.1",
    "resolve-url-loader": "^1.4.3",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^1.1.0"
  },
  "dependencies": {
    "peerjs": "^0.3.14",
    "ramda": "^0.22.1",
    "redux": "^3.6.0",
    "redux-observable": "^0.12.2",
    "redux-thunk": "^2.1.0"
  }
}
